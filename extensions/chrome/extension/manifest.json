{
  "manifest_version": 2,

  "name": "Listboard.it",
  "description": "This extension is to sync tabs with Listboard.it.",
  "version": "1.0.1",

  "permissions": [
    "tabs",
    "storage",
    "*://*.listboard.it/"
  ],

  "background": {
    "scripts": ["jquery.min.js", "background.js"]
  },

  "browser_action": {
    "default_icon": "icon.ico"
  },

  "web_accessible_resources": [ "manifest.json" ]
}